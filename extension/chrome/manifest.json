{
    "name": "IP Geolocator",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "description": "Shows detailed information about your IP and location.",
    "background": {"page": "background.html"},
    "incognito": "spanning",
    "minimum_chrome_version": "25",
    "offline_enabled": false,
    "options_page": "options.html",
    "permissions": [
        "https://aykutcevik.com/"
    ],
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html",
        "default_title": "IP Geolocator"
    }
}
